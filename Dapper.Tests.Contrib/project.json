  "commands": {
    "test": "xunit.runner.dnx"
  },
  "compilationOptions": {
    "warningsAsErrors": true
  },
  "frameworks": {
    "net40": {
      "frameworkAssemblies": {
        "System.Configuration": "4.0.0.0",
        "System.Data": "4.0.0.0",
