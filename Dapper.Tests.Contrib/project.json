  "version": "1.0.0-*",
  "copyright": "2015 Stack Exchange, Inc.",
  "dependencies": {
    "Dapper": {
      "version": "1.50-*",
      "target": "project"
    },
    "Dapper.Contrib": {
      "version": "1.50-*",
      "target": "project"
    },
    "Dapper.SqlBuilder": {
      "version": "1.50-*",
      "target": "project"
    }
  },
  "compile": [
    "**/*.cs",
    "../Dapper.Tests/Assert.cs",
    "../Dapper/TypeExtensions.cs"
