{
  "authors": [ "Sam Saffron" ],
  "owners": [ "samsaffron" ],
  "projectUrl": "https://github.com/StackExchange/dapper-dot-net",
  "licenseUrl": "http://www.apache.org/licenses/LICENSE-2.0",
  "summary": "A demo is available at https://gist.github.com/1599013",
  "description": "Trivial micro-orm implemented on Dapper, provides with CRUD helpers.",
  "version": "0.1.3-beta1",
  "title": "Dapper.Rainbow",
  "tags": [ "orm", "sql", "micro-orm" ],
  "copyright": "2012 Sam Saffron",
  "dependencies": {
    "Dapper": {
      "version": "1.50-*",
      "target": "project"
    }
  },
  "compile": [
    "**/*.cs",
    "../Dapper/TypeExtensions.cs"
  ],
  "compilationOptions": {
    "warningsAsErrors": true
  },
  "frameworks": {
    "net40": {
      "frameworkAssemblies": {
        "System.Configuration": "4.0.0.0",
        "System.Data": "4.0.0.0",
        "System.Data.Linq": "4.0.0.0"
      }
    },
    "net45": {
      "compilationOptions": {
        "define": [ "ASYNC" ]
      },
      "frameworkAssemblies": {
        "System.Configuration": "4.0.0.0",
        "System.Data": "4.0.0.0",
        "System.Data.Linq": "4.0.0.0"
      }
    },
    "netstandard1.3": {
      "compilationOptions": {
        "define": [ "ASYNC", "COREFX" ]
      },
        "dependencies": {
            "Microsoft.CSharp": "4.0.1-rc2-24015"
        }
    }
  }
}